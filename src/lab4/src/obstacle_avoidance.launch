<launch>
    <arg name="nodelet_manager_name" value="nodelet_manager"/>
    <arg name="config_file" value="$(find lab4)/param/nodes_priority.yaml"/>

    <node pkg="nodelet" type="nodelet" name="$(arg nodelet_manager_name)" args="manager"/>
    <include file="$(find yocs_cmd_vel_mux)/launch/cmd_vel_mux.launch">
        <arg name="nodelet_manager_name" value="$(arg nodelet_manager_name)"/>
        <arg name="config_file" value="$(arg config_file)"/>
    </include>
    <node name="turtlebot_teleop_key" pkg="turtlebot_teleop" type="turtlebot_teleop_key" output="screen"> 
        <remap from="/turtlebot_teleop_key/cmd_vel" to="/keyop/cmd_vel"/>
    </node>
    <node name="obstacle_avoidance" pkg="lab4" type="obstacle_avoidance.py" output="screen" />
</launch>
